{
    "name": "sorin/lendinvest",
    "authors": [
        {
            "name": "Sorin Popescu",
            "email": "popescusorinbogdan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^0.9.2",
        "phpstan/phpstan": "^0.8.3",
        "phpunit/phpunit": "^6.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "preferred-install": "dist",
        "sort-packages": true
    }
}
